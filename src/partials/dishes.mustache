<div class="row">
    {{#dishes}}
        <div class="col-4">
            <div class="card" style="margin-bottom: 20px;">
                <img class="card-img-top" src="{{image}}" style="height: 225px;" />
                <div class="card-block">
                    <h4 class="card-title" style="min-height: 50px;">{{title}}</h4>
                    <p class="card-text">{{shortInstructions}}</p>
                    <a href="./create/{{id}}" class="btn btn-success">Info</a>
                </div>
            </div>
        </div>
    {{/dishes}}
</div>
