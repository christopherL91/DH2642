        "watch": "nodemon --exec 'npm run build' --watch src"
    },
    "dependencies": {
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-latest": "^6.22.0",
        "browser-sync": "^2.18.7",
        "mustache": "^2.3.0",
        "nodemon": "^1.11.0",
        "npm-run-all": "^4.0.1",
        "page": "^1.7.1",
        "ramda": "^0.23.0",
        "rollup": "^0.41.4",
        "rollup-plugin-babel": "^2.7.1",
        "rollup-plugin-commonjs": "^7.0.0",
        "rollup-plugin-node-resolve": "^2.0.0"
    }
}
