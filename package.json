{
    "name": "DH2642",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "npm run build && npm-run-all --parallel watch browse",
        "browse": "browser-sync start --s --index 'index.html' --files 'src/partials/**/*.mustache, src/**/*.js' --no-notify",
